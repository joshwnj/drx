{
  "name": "drx",
  "version": "0.11.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "babel -d lib src",
    "lint": "standard",
    "prepublishOnly": "npm run build && npm run umd",
    "umd": "npm run umd:prep && npm run umd:build && npm run umd:minify",
    "umd:prep": "rm -rf dist && mkdir dist",
    "umd:build": "browserify --standalone drx -o dist/drx.js lib/index.js",
    "umd:minify": "uglifyjs -o dist/drx.min.js dist/drx.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwnj/drx.git"
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwnj/drx/issues"
  },
  "homepage": "https://github.com/joshwnj/drx#readme",
  "peerDependencies": {
    "react": ">= 0.14.0 < 17.0.0-0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "jest": "^20.0.4",
    "react": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "standard": "^10.0.3",
    "uglify-js": "^3.0.28"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ]
